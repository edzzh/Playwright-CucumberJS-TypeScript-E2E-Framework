{
  "name": "playwright-cucumber-ts-framework",
  "version": "1.0.0",
  "description": "E2E Testing Framework using Playwright, CucumberJS and Typescript",
  "main": "cucumber.js",
  "scripts": {
    "test": "npx cucumber-js -p runner",
    "test:tags": "npx cucumber-js -p runner --tags",
    "test:generator": "npx playwright codegen process.env.URL"
  },
  "keywords": [
    "playwright",
    "cucumber",
    "typescript"
  ],
  "author": "Edgars Hartmanis",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^9.1.0",
    "@playwright/test": "^1.32.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "winston": "^3.8.2"
  }
}
